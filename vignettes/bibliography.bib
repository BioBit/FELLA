file = {:home/sergi/all/devel/articles/mendeley/2016/Towards Improving Point-of-Care Diagnosis of Non-malaria Febrile Illness A Metabolomics Approach.pdf:pdf},
isbn = {1935-2735 (Electronic) 1935-2727 (Linking)},
issn = {19352735},
journal = {PLoS Neglected Tropical Diseases},
keywords = {MTBLS315,main{\_}paper},
