@article{malaria_phenylalanine_sengupta,
abstract = {BACKGROUND: Plasmodium vivax is responsible for the majority of malarial infection in the Indian subcontinent. This species of the parasite is generally believed to cause a relatively benign form of the disease. However, recent reports from different parts of the world indicate that vivax malaria can also have severe manifestation. Host response to the parasite invasion is thought to be an important factor in determining the severity of manifestation. In this paper, attempt was made to determine the host metabolic response associated with P. vivax infection by means of NMR spectroscopy-based metabonomic techniques in an attempt to better understand the disease pathology.$\backslash$n$\backslash$nMETHODS: NMR spectroscopy of urine samples from P. vivax-infected patients, healthy individuals and non-malarial fever patients were carried out followed by multivariate statistical analysis. Two data analysis techniques were employed, namely, Principal Component Analysis [PCA] and Orthogonal Projection to Latent Structure Discriminant Analysis [OPLS-DA]. Several NMR signals from the urinary metabolites were further selected for univariate comparison among the classes.$\backslash$n$\backslash$nRESULTS: The urine metabolic profiles of P. vivax-infected patients were distinct from those of healthy individuals as well as of non-malarial fever patients. A highly predictive model was constructed from urine profile of malarial and non-malarial fever patients. Several metabolites were found to be varying significantly across these cohorts. Urinary ornithine seems to have the potential to be used as biomarkers of vivax malaria. An increasing trend in pipecolic acid was also observed. The results suggest impairment in the functioning of liver as well as impairment in urea cycle.$\backslash$n$\backslash$nCONCLUSIONS: The results open up a possibility of non-invasive analysis and diagnosis of P. vivax using urine metabolic profile. Distinct variations in certain metabolites were recorded, and amongst these, ornithine may have the potential of being used as biomarker of malaria. Pipecolic acid also showed increasing trend in the malaria patient compared to the other groups.},
author = {Sengupta, Arjun and Ghosh, Soumita and Basant, Angika and Malusare, Suhas and Johri, Parul and Pathak, Sulabha and Sharma, Shobhona and Sonawat, Haripalsingh M},
doi = {10.1186/1475-2875-10-384},
file = {:home/sergi/all/devel/articles/mendeley/2011/Global host metabolic response to Plasmodium vivax infection a 1H NMR based urinary metabonomic study.pdf:pdf},
isbn = {1475-2875 (Electronic)$\backslash$r1475-2875 (Linking)},
issn = {1475-2875},
journal = {Malaria journal},
keywords = {Adult,Biological Markers,Biological Markers: metabolism,Biological Markers: urine,Case-Control Studies,Humans,MTBLS315,Magnetic Resonance Spectroscopy,Malaria,Male,Metabolome,Metabolomics,Metabolomics: methods,Middle Aged,Multivariate Analysis,Ornithine,Ornithine: metabolism,Ornithine: urine,Pipecolic Acids,Pipecolic Acids: metabolism,Pipecolic Acids: urine,Plasmodium vivax,Plasmodium vivax: pathogenicity,Predictive Value of Tests,Specimen Handling,Specimen Handling: methods,Vivax,Vivax: metabolism,Vivax: parasitology,Vivax: urine,phenylalanine,phenylpyruvate},
mendeley-tags = {MTBLS315,phenylalanine,phenylpyruvate},
